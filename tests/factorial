/*Result: 720*/


/* Main entry point */
int main() {

    int fact(int n) {
    	int inner_fact(int n, int a) {
    		/* Mention how IF stmts need to be in curlies */
    		if (n==0) return a;
    		return inner_fact(n-1, a * n);
    	}
    	return inner_fact(n, 1);
    }

    print_int(fact(6));
	return fact(6);
}